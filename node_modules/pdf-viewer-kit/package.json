{
  "name": "pdf-viewer-kit",
  "description": "PDF-Viewer-Kit is a versatile, framework-agnostic library designed to render, manipulate, and interact with PDF documents seamlessly. Whether you're building a React, Next.js, Angular, or any other frontend application, PDF-Kit provides you with a well-structured, modular, and feature-rich solution for embedding PDF viewers into your web applications.",
  "main": "dist/pdfkit.cjs.js",
  "module": "dist/pdfkit.es.js",
  "types": "dist/index.d.ts",
  "author": "Aman Kumar",
  "private": false,
  "version": "0.2.0-canvas-pooling.1",
  "keywords": [
    "pdf",
    "react",
    "viewer"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AmanKrr/pdf-kit.git"
  },
  "scripts": {
    "semantic-release": "semantic-release",
    "doc": "jsdoc -r -d docs src",
    "doc-1": "jsdoc -c jsdoc.json",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "lodash": "^4.17.21",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "5.2.133"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "11.0.2",
    "@semantic-release/npm": "12.0.1",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@types/lodash": "^4.17.17",
    "@types/node": "20.11.6",
    "semantic-release": "24.2.4",
    "typedoc": "0.28.4",
    "typedoc-github-theme": "^0.3.0",
    "typedoc-plugin-markdown": "^4.6.3",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "release": {
    "repositoryUrl": "https://github.com/AmanKrr/pdf-kit.git",
    "branches": [
      "main",
      {
        "name": "canvas-pooling",
        "prerelease": true
      }
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "gitRawCommitOpts": {
            "from": "v0.1.0"
          }
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "npm run build"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ]
        }
      ]
    ],
    "ci": false,
    "dryRun": false
  }
}
